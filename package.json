{
  "name": "fastenglish",
  "version": "0.2.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "budo ./js/index.js:dist/bundle.js -dir lib/ --live -- -t node-lessify -t babelify",
    "clean": "rimraf ./dist/*",
    "build": "npm run html & npm run js & npm run images",
    "rebuild": "npm run clean && npm run build",
    "html": "cp ./index.html ./dist/",
    "js": "browserify ./index.js -t node-lessify -t babelify | uglifyjs > ./dist/index.js",
    "images": "cp ./favicon.ico ./dist/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:conradoqg/fastenglish.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conradoqg/fastenglish/issues"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "browserify-css": "^0.10.1",
    "budo": "^10.0.3",
    "node-lessify": "^0.1.4",
    "rimraf": "^2.6.1",
    "uglify-js": "^2.8.22"
  },
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  }
